@inject NavigationManager NavigationManager
@* To add Interactivity now need to import render mode and add @rendermode *@
@rendermode InteractiveServer

<button @onclick="NavigateToEditGamePage" class="btn btn-primary">Edit Game</button>

@code {
	[Parameter]
	public int Id { get; set; }

	void NavigateToEditGamePage()
	{
		NavigationManager.NavigateTo($"/edit-game/{Id}");
	}
}
